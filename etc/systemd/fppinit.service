[Unit]
Description=Inialization routines for FPP
After=connman.target home-fpp-media.mount
Before=network.target multi-user.target apache2.target
Wants=connman.target network-pre.target home-fpp-media.mount
DefaultDependencies=no

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/opt/fpp/scripts/fpp_init start


[Install]
WantedBy=network.target multi-user.target
